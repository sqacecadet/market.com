<%= simple_form_for @type_partner, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :type %>
  <%= error_span(@type_partner[:type]) %>
  <%= f.input :description %>
  <%= error_span(@type_partner[:description]) %>
  <%= f.input :documents_id %>
  <%= error_span(@type_partner[:documents_id]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                type_partners_path, :class => 'btn btn-default' %>
<% end %>
