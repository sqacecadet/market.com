<%= simple_form_for @partner, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>

  <%= f.input :price %>

  <%= f.input :TypePartner_id %>

  <%= f.input :documents_id %>

  <%= f.button :submit, :class => 'btn-primary' %>

  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                partners_path, :class => 'btn btn-default' %>
<% end %>
